{
    "workbench.activityBar.visible": false,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 3000,
    "files.autoGuessEncoding": true,
    "files.trimTrailingWhitespace": true,
    "editor.fontSize": 11.5,
    "terminal.integrated.fontSize": 11.5,
    "editor.wordWrap": "on",
    "editor.minimap.enabled": false,
    "editor.fontFamily": "'源ノ角ゴシック Code JP'",
    "editor.tabCompletion": "on",
    "editor.snippetSuggestions": "top",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.matchBrackets": true,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "trailing-spaces.deleteModifiedLinesOnly": true,
    "git.path": "C:\\Programs\\GitSDK\\mingw64\\bin\\git.exe",
    "C_Cpp.default.compilerPath": "clang++",
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.default.intelliSenseMode": "clang-x64",
    "C_Cpp.errorSquiggles": "Enabled",
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.clang_format_style": "file",
    "python.pythonPath": "C:\\Programs\\Python37\\python.exe",
    "python.formatting.provider": "autopep8",
    "python.formatting.autopep8Args": [
        "--aggressive",
        "--aggressive"
    ],
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "bracketPairColorizer.forceUniqueOpeningColor": true,
    "bracketPairColorizer.colorMode": "Consecutive",
    "bracketPairColorizer.showBracketsInGutter": true,
    "indentRainbow.includedLanguages": [
        "nim",
        "nims",
        "python",
        "shellscript",
        "fsharp"
    ],
    "indentRainbow.colors": [
        "rgba(255,202,40,0.5)",
        "rgba(182,187,106,0.5)",
        "rgba(66,165,245,0.5)",
        "rgba(239,83,80,0.5)",
        "rgba(120,144,156,0.5)"
    ],
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "foreground": "#F5F5F5",
            "background": "#C62828",
            "icon": "alert",
            "iconColour": "#C62828",
            "type": "text-and-comment"
        },
        "FIXME": {
            "foreground": "#F5F5F5",
            "background": "#F9A825",
            "icon": "beaker",
            "iconColour": "#F5F5F5",
            "type": "text",
        }
    },
    "cmake.configureOnOpen": false,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.incsearch": true,
    "vim.visualstar": true,
    "vim.leader": "<space>",
    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.whichwrap": "h,l,<,>,[,]",
    "vim.foldfix": true,
    "vim.camelCaseMotion.enable": true,
    "vim.handleKeys": {
        "<C-a>": false, // 全選択
        "<C-f>": false, // 検索
        "<C-c>": false, // コピー
        "<C-x>": false, // カット
        "<C-i>": false, // 列選択。別に要らないけどvim側の機能がタブとかいうもっといらない物体
        "<C-y>": false, // Redo
        "<C-z>": false, // Undo
        "<C-q>": false, // サイドバー周り
        "<C-k>": false, // vscodeのプレフィックス
